<% @detailed_stats["os_groups"].map do |stat_name, stat_value| %>
  <% stat_percentage = (stat_value["count"].to_f / @detailed_stats["os_groups_total"].to_f * 100) %>
  <%= render partial: "stats/host/os_groups", locals: { stat_name: stat_name, stat_value: stat_value, stat_percentage: stat_percentage }%>
<% end %>
