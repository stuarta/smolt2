<div class="accordion">
  <details name="timezone_breakdown_accordion">
    <summary>Timezones</summary>
    <p>
      <table class="table">
        <caption>
          Timezones
        </caption>
        <thead>
          <th>Timezone</th>
          <th>Count</th>
          <th>% of total</th>
          <th></th>
        </thead>
        <%= render partial: "simple_stats_rows", locals: { stats: @mythtv_stats["timezone"] }%>
      </table>
    </p>
  </details>
</div>
